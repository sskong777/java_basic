package map;

import java.util.*;

public class EX01 {

	public static void main(String[] args) {

		HashMap<Integer, String> map = new HashMap<>();
		
		map.put(new Integer(1), new String("홍길동"));
		map.put(2, "성춘향");
		map.put(3, "이순신");
		map.put(4, "이몽룡");
		map.put(1, "홍길순");
	
		System.out.println(map.size());
		
		System.out.println(map);
	}

}
