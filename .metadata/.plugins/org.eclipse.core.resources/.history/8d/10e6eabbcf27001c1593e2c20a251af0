package io;

import java.io.*;

public class EX03 {

	public static void main(String[] args) throws Exception{
		
		File dir = new File("C:" + File.separator + "aaa");
		File file = new File(dir,"aaa.txt");
	
		FileOutputStream fos = new FileOutputStream(file);
		//파일이 없으면 만든다.
		
		fos.write(65);

	}

}
