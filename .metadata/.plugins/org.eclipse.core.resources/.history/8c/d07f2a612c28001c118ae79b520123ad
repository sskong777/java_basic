package generic;

class Box <E>{
	
	private E obj;
	
	public E getObj() {
		return obj;
	}
	
	public void setObj(E obj) {
		this.obj = obj;
	}
	
}

public class EX02 {

	public static void main(String[] args) {

		Box box = new Box();
		
		box.setObj("안녕하세요");	//타입이 object
		
		String str = (String)box.getObj();	//강제 다운캐스팅
		System.out.println(str);
//		
	
	
	}

}
